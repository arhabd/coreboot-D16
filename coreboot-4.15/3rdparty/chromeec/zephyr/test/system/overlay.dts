/ {
	named-bbram-regions {
		compatible = "named-bbram-regions";
		pd0 {
			offset = <0x00>;
			size = <0x01>;
		};
		pd1 {
			offset = <0x01>;
			size = <0x02>;
		};
		try_slot {
			offset = <0x03>;
			size = <0x04>;
		};
		pd2 {
			offset = <0x07>;
			size = <0x05>;
		};
	};
};
